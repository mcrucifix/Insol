#### http://www.people.fas.harvard.edu/~phuybers/Progression/Averages.txt

local({
  fpath <- file.path('..', 'orig', 'Averages.txt')
  d18OHuy <<- read.table(fpath, skip = 9, header = TRUE)
})

d18OHuy$CE <- 1e3 * (1.950 - d18OHuy$Age)
d18OHuy$delta18O <- d18OHuy$Records
d18OHuy$Records <- NULL

d18OHuy <- d18OHuy[order(d18OHuy$CE), ]
